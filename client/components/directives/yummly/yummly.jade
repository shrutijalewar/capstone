h3 Search
.row
  .col-xs-6
    #yumRs
      .yumR(ng-repeat= 'yumR in yumRecipes')
        h3 {{yumR.recipeName}}
        img(ng-src='{{yumR.smallImageUrls[0]}}')
        h5 Course {{yumR.attributes.course[0]}}
        button.btn.btn-primary(ng-click='getDirections(yumR.id)') Get Directions
        button.btn.btn-danger(ng-click='saveLink(yumR.id)') Save Link

